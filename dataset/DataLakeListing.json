{
	"name": "DataLakeListing",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureDataLakeStorage1",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"postcode": {
				"type": "string",
				"defaultValue": "XXXX"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(convertTimeZone(utcnow(), 'UTC', 'AUS Eastern Standard Time'), '.json')",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('postcode=',dataset().postcode)",
					"type": "Expression"
				},
				"fileSystem": "listing"
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}